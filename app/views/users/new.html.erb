<h1>Signup Form</h1>
<%= form_for(@user) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :nausea %>
  <%= f.text_field :nausea %>
  <%= f.label :happiness %>
  <%= f.text_field :happiness %>
  <%= f.label :tickets %>
  <%= f.text_field :tickets %>
  <%= f.label :height %>
  <%= f.text_field :height %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label :ADMIN %>
  <%= f.check_box :admin %>
  <%= f.submit %>
<% end %>

<%= form_tag %>
